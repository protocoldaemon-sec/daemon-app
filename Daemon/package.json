{
  "name": "fusion-starter",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build && node fix-mobile-build.js",
    "build:server": "vite build --config vite.config.server.ts",
    "build:analyze": "vite build && node fix-mobile-build.js && open dist/bundle-analysis.html",
    "build:client-only": "vite build",
    "start": "node dist/server/node-build.mjs",
    "test": "vitest --run",
    "test:seed-vault": "vitest --run --config vitest.seed-vault.config.ts",
    "test:seed-vault:watch": "vitest --watch --config vitest.seed-vault.config.ts",
    "test:seed-vault:coverage": "vitest --coverage --config vitest.seed-vault.config.ts",
    "format.fix": "prettier --write .",
    "typecheck": "tsc",
    "mobile:build": "npm run build:client && npx cap sync android",
    "mobile:open": "npx cap open android",
    "mobile:run": "npx cap run android",
    "mobile:build:apk": "cd android && ./gradlew assembleDebug",
    "generate:icons": "node generate-android-icons.js"
  },
